{
  "name": "SleepingOwlAdmin",
  "version": "0.1",
  "dependencies": {
    "jquery": "latest",
    "underscore": "latest",
    "bootstrap": "latest",
    "bootbox": "latest",
    "eonasdan-bootstrap-datetimepicker": "latest",
    "fontawesome": "latest",
    "moment": "latest",
    "nestable": "latest",
    "noty": "latest",
    "Sortable": "latest",
    "select2": "latest",
    "flow.js": "latest",
    "ekko-lightbox": "latest",
    "metisMenu": "latest",
    "datatables": "latest",
    "colorbox": "latest",
    "admin-lte": "latest",
    "x-editable": "latest"
  },
  "install": {
    "base": "public/default/libs",
    "path": {
      "js": "{name}/js",
      "map": "{name}/js",
      "less": "{name}/less",
      "css": "{name}/css",
      "eot": "{name}/fonts",
      "svg": "{name}/fonts",
      "ttf": "{name}/fonts",
      "woff": "{name}/fonts",
      "woff2": "{name}/fonts",
      "otf": "{name}/fonts",
      "gif": "{name}/images",
      "png": "{name}/images",
      "jpg": "{name}/images"
    },
    "sources": {
      "bootstrap": [
        "bower_components/bootstrap/dist/css/**",
        "bower_components/bootstrap/dist/js/**",
        "bower_components/bootstrap/fonts/**",
        "bower_components/bootstrap/less/**"
      ],
      "datetimepicker": [
        "bower_components/datetimepicker/jquery.datetimepicker.css",
        "bower_components/datetimepicker/build/jquery.datetimepicker.full.min.js"
      ],
      "fontawesome": [
        "bower_components/fontawesome/css/**",
        "bower_components/fontawesome/fonts/**",
        "bower_components/fontawesome/less/**"
      ],
      "jquery": [
        "bower_components/jquery/dist/jquery.min.js",
        "bower_components/jquery/dist/jquery.min.map"
      ],
      "moment": [
        "bower_components/moment/min/moment-with-locales.min.js"
      ],
      "nestable": [
        "bower_components/nestable/jquery.nestable.js"
      ],
      "noty": [
        "bower_components/noty/js/noty/packaged/*.*"
      ],
      "select2": [
        "bower_components/select2/dist/css/*.*",
        "bower_components/select2/dist/js/**"
      ],
      "Sortable": [
        "bower_components/Sortable/jquery.binding.js",
        "bower_components/Sortable/Sortable.min.js"
      ],
      "flow.js": [
        "bower_components/flow.js/dist/flow.min.js"
      ],
      "metisMenu": [
        "bower_components/metisMenu/dist/metisMenu.min.css",
        "bower_components/metisMenu/dist/metisMenu.min.js"
      ],
      "datatables": [
        "bower_components/datatables/media/js/jquery.dataTables.min.js",
        "bower_components/datatables/media/js/dataTables.bootstrap.min.js",
        "bower_components/datatables/media/css/dataTables.bootstrap.min.css",
        "bower_components/datatables/media/css/jquery.dataTables.min.css"
      ],
      "admin-lte": [
        "bower_components/admin-lte/build/less/**",
        "bower_components/admin-lte/dist/js/**"
      ],
      "colorbox": [
        "bower_components/colorbox/jquery.colorbox.js",
        "bower_components/colorbox/jquery.colorbox-min.js",
        "bower_components/colorbox/example3/images/*.*"
      ],
      "x-editable": [
        "bower_components/x-editable/dist/bootstrap3-editable/js/*.*",
        "bower_components/x-editable/dist/bootstrap3-editable/css/*.*",
        "bower_components/x-editable/dist/bootstrap3-editable/img/*.*"
      ]
    }
  }
}